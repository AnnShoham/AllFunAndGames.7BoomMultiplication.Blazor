@page "/MuliplicationTable"
<h3>MuliplicationTable</h3>


    <div class="Multiplication">
        <table border="1">
            @for (int i = 1; i<= 10; i++)
            {
            <tr border="1">
                @for (int k = i; k <= i * 10; k=k+i)
                {  
                  {
                    <td border="1"> @k </td>
                  }
                }
            </tr>
            }

        </table>
    </div> 

    <style>
        body{
        background-image: url('/images/mathBackground.JPEG')
        }
    </style>

